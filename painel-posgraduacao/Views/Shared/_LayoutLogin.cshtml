@{
    var resultPartialView = string.Empty;
    long PerfilId = Convert.ToInt64(ViewData["PerfilId"]);

    if(PerfilId == 1){
        resultPartialView = "_ResultsMestradoRegular";
    }
    else if(PerfilId == 2){
        resultPartialView = "_ResultsDoutorado";
    }
    else if (PerfilId == 3)
    {
        resultPartialView = "_ResultsMestradoEspecial";
    }

}
